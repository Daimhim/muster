<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tv_plugin_name_pm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_size_12"
        android:layout_marginTop="@dimen/dimen_size_12"
        android:layout_marginRight="@dimen/dimen_size_12"
        android:text="微信"
        android:textColor="@color/cl_333333"
        android:textSize="@dimen/dimen_text_size_16"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:text="com.tencent.mm"
        android:id="@+id/tv_package_name_pm"
        android:layout_marginLeft="@dimen/dimen_size_12"
        android:layout_marginRight="@dimen/dimen_size_12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/cl_666666"
        android:textSize="@dimen/dimen_text_size_12"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_plugin_name_pm" />
</android.support.constraint.ConstraintLayout>